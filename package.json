{
	"name": "cssfmt",
	"description": "The tool applying CSS formatting guidelines.",
	"version": "0.0.1",
	"publisher": "Ilya Verbitskiy",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cssfmt"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "cssfmt",
			"title": "Format CSS"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}